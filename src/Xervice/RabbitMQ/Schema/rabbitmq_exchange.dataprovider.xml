<?xml version="1.0"?>

<DataProviders
  xmlns="xervice:dataprovider-01"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="xervice:dataprovider-01 http://static.xervice.online/schema/dataprovider.schema.xsd"
>

    <DataProvider name="RabbitMqExchange">
        <DataElement name="Name" type="string"/>
        <DataElement name="Type" type="string"/>
        <DataElement name="Passive" default="false" type="bool"/>
        <DataElement name="Durable" default="false" type="bool"/>
        <DataElement name="AutoDelete" default="true" type="bool"/>
        <DataElement name="Internal" default="false" type="bool"/>
        <DataElement name="NoWait" default="false" type="bool"/>
        <DataElement name="Argument" default="[]" type="array"/>
        <DataElement name="Ticket" allownull="true" type="int"/>
    </DataProvider>

</DataProviders>