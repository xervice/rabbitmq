<?xml version="1.0"?>

<DataProviders
  xmlns="xervice:dataprovider-01"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="xervice:dataprovider-01 http://static.xervice.online/schema/dataprovider.schema.xsd"
>

    <DataProvider name="RabbitMqConsumerConfig">
        <DataElement name="Tag" default="''" type="string"/>
        <DataElement name="NoLocal" default="false" type="bool"/>
        <DataElement name="NoAck" default="false" type="bool"/>
        <DataElement name="Exclusive" default="false" type="bool"/>
        <DataElement name="NoWait" default="false" type="bool"/>
        <DataElement name="Ticket" type="string" allownull="true"/>
        <DataElement name="Arguments" default="[]" type="array"/>
    </DataProvider>

</DataProviders>